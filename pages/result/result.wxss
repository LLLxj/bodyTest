/* pages/result/result.wxss */
.wrap{
  width: 100%;
  position:relative;
}
.content{
  background: #F9F9F9;
  padding: 15rpx 0;
}
.list{
  width:84%;
  background: #ffffff;
  position:relative;
  margin:0 auto;
  padding:30rpx;
  border-radius:5px;
  margin-top:30rpx;
}
.showImg{
  width:180rpx;
  height:180rpx;
  float:left;
}
.showImg image{
  width:180rpx;
  height:180rpx!important;
  border-radius:5px;
}
.showInfo{
  float: left;
  margin-left: 5%;
}
.showNum .getNum{
  color:#333;
  font-size: 34rpx;
  float: left;
}
.warnInfo{
  display:block;
  width:100%;
  margin-top:30rpx;
  font-size: 24rpx!important;
  color:#676767;
}
.dateInfo{
  display:block;
  margin-top:45rpx;
  font-size: 24rpx!important;
  color:#9f9f9f;
}
.showAd{
  margin-left: 26rpx;
  font-size: 20rpx!important;
  color:#fff!important;
  border-radius:5px;
  padding:5rpx 25rpx;
  margin-top:5rpx;
}
.clear{
  clear:both;
}
.cornerTitle{
  position:absolute;
  width:100rpx;
  height:100rpx;
  top:0;
  right:0rpx;
  /* padding: 30rpx 0 10rpx 0; */
}
.cornerTitle image{
  width:100%;
}

